
public class Graph {
	int[][] matrix;

	Graph(int size) {
		this.matrix = new int[size][size];
	}

	public void addNode(Node node) {
	}

	public void addEdge(int src, int dst) {
		this.matrix[src][dst] = 1;
	}

	public boolean checkEdge(int src, int dst) {
		if (this.matrix[src][dst] == 1)
			return true;
		return false;
	}
}
