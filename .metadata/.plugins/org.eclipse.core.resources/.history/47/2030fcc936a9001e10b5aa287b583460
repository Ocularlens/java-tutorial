	public static void builtIn() {
		int[] array = new int[100];
		int target = 150;
		
		
		for (int ctr = 0; ctr < array.length; ctr++) {
			array[ctr] = ctr;
		}
		
		int index = Arrays.binarySearch(array, target);
		
		System.out.println(index);
	}
	
